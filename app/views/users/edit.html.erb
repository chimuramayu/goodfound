<div class="main">
	<div class="container">
	<%= form_for(@user) do |f| %>
		<div>
			<%= attachment_image_tag @user, :icon_image, :fill, 250, 250, class:"icon-image" %><br>
		</div>
		<%= f.attachment_field :icon_image %>
		<div class="">
			<%= f.label :name, "名前" %><br>
			<%= f.text_field :name %>
		</div>
		<div class="">
			<%= f.label :profile, "プロフィール" %><br>
			<%= f.text_area :profile %>
		</div>
		<%= f.submit "更新" %>
	<% end %>
</div>
</div>